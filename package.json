{
  "name": "@adobe/reactor-extension-cloud-connector-edge",
  "version": "1.2.3",
  "description": "Adobe Experience Platform event forwarding extension to make fetch calls to third-party endpoints.",
  "author": {
    "name": "Adobe Systems",
    "url": "http://adobe.com",
    "email": "reactor@adobe.com"
  },
  "license": "Apache 2.0",
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:adobe/reactor-extension-cloud-connector-edge.git"
  },
  "scripts": {
    "build": "rm -rf dist && webpack --mode production",
    "build:watch": "webpack --mode development --watch",
    "sandbox": "concurrently -k -n webpack,sandbox -c \"bgBlue,bgMagenta\" \"npm run build:watch\" \"reactor-sandbox\"",
    "package": "npm run build && npx @adobe/reactor-packager",
    "prepack": "npm test && npm run build",
    "lint": "eslint 'src/**'",
    "format": "pretty-quick",
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "update": "rm -rf node_modules package-lock.json && npm i",
    "prepare": "husky"
  },
  "lint-staged": {
    "*.js": [
      "eslint --cache --fix"
    ]
  },
  "dependencies": {
    "@adobe/react-spectrum": "^3.40.1",
    "@spectrum-icons/illustrations": "^3.6.20",
    "@spectrum-icons/workflow": "^4.2.19",
    "classnames": "^2.5.1",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-hook-form": "^7.55.0",
    "react-stately": "^3.36.1"
  },
  "devDependencies": {
    "@adobe/reactor-sandbox": "^13.2.0",
    "@babel/core": "^7.26.10",
    "@babel/eslint-parser": "^7.27.0",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-syntax-import-assertions": "^7.26.0",
    "@babel/plugin-transform-runtime": "^7.26.10",
    "@babel/preset-env": "^7.26.9",
    "@babel/preset-react": "^7.26.3",
    "@babel/runtime": "^7.27.0",
    "@eslint/js": "^9.24.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "babel-jest": "^29.7.0",
    "babel-loader": "^10.0.0",
    "camelcase": "^8.0.0",
    "capitalize": "^2.0.4",
    "concurrently": "^9.1.2",
    "css-loader": "^7.1.2",
    "eslint": "^9.24.0",
    "eslint-config-prettier": "^10.1.1",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-prettier": "^5.2.6",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "file-loader": "^6.2.0",
    "fs-extra": "^11.3.0",
    "globals": "^16.0.0",
    "html-webpack-plugin": "^5.6.3",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "lint-staged": "^15.5.0",
    "prettier": "^3.5.3",
    "pretty-quick": "^4.1.1",
    "style-loader": "^4.0.0",
    "stylus": "^0.64.0",
    "stylus-loader": "^8.1.1",
    "url-loader": "^4.1.1",
    "webpack": "^5.99.4",
    "webpack-cli": "^6.0.1",
    "yargs": "^17.7.2"
  }
}
