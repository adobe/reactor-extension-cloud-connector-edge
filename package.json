{
  "name": "@adobe/reactor-extension-cloud-connector-edge",
  "version": "1.0.0",
  "description": "Adobe Experience Platform Launch Server Side that allows you to make fetch calls to third-party endpoints.",
  "author": {
    "name": "Adobe Systems",
    "url": "http://adobe.com",
    "email": "reactor@adobe.com"
  },
  "license": "Apache 2.0",
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:adobe/reactor-extension-cloud-connector-edge.git"
  },
  "scripts": {
    "build": "rm -rf dist && webpack --mode production",
    "build:watch": "webpack --mode development --watch",
    "sandbox": "concurrently -k -n webpack,sandbox -c \"bgBlue,bgMagenta\" \"npm run build:watch\" \"reactor-sandbox\"",
    "package": "npm run build && npx @adobe/reactor-packager",
    "prepack": "npm test && npm run build",
    "lint": "eslint 'src/**'",
    "format": "pretty-quick",
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "update": "rm -rf node_modules package-lock.json && npm i"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged && lint-staged",
      "pre-push": "npm test"
    }
  },
  "lint-staged": {
    "*.js": [
      "npm run lint -- --fix"
    ]
  },
  "dependencies": {
    "@adobe/react-spectrum": "^3.6.0",
    "@react-spectrum/combobox": "^3.0.0-alpha.0",
    "@react-spectrum/tabs": "^3.0.0-alpha.3",
    "@spectrum-icons/illustrations": "^3.2.1",
    "@spectrum-icons/workflow": "^3.2.0",
    "classnames": "^2.2.6",
    "prop-types": "^15.7.2",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-hook-form": "^6.13.1"
  },
  "devDependencies": {
    "@adobe/reactor-sandbox": "^11.0.2",
    "@babel/core": "^7.12.10",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/plugin-transform-runtime": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "@babel/preset-react": "^7.12.10",
    "@babel/runtime": "^7.12.5",
    "@testing-library/jest-dom": "^5.11.6",
    "@testing-library/react": "^11.2.2",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.6.3",
    "babel-loader": "^8.2.2",
    "camelcase": "^6.2.0",
    "capitalize": "^2.0.3",
    "concurrently": "^5.3.0",
    "css-loader": "^5.0.1",
    "eslint": "^7.16.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^7.0.0",
    "eslint-plugin-header": "^3.1.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest-dom": "^3.6.4",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.3.0",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-testing-library": "^3.10.1",
    "file-loader": "^6.2.0",
    "fs-extra": "^9.0.1",
    "html-webpack-plugin": "^5.0.0-alpha.15",
    "husky": "^4.3.6",
    "jest": "^26.6.3",
    "jest-runner-eslint": "^0.10.0",
    "lint-staged": "^10.5.3",
    "prettier": "^2.2.1",
    "pretty-quick": "^3.1.0",
    "style-loader": "^2.0.0",
    "stylus": "^0.54.8",
    "stylus-loader": "^4.3.1",
    "url-loader": "^4.1.1",
    "webpack": "^5.11.0",
    "webpack-cli": "^4.2.0",
    "yargs": "^16.2.0"
  }
}
