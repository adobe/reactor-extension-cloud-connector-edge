{
  "name": "@adobe/reactor-extension-cloud-connector-edge",
  "version": "1.0.0",
  "description": "Adobe Experience Platform event forwarding extension to make fetch calls to third-party endpoints.",
  "author": {
    "name": "Adobe Systems",
    "url": "http://adobe.com",
    "email": "reactor@adobe.com"
  },
  "license": "Apache 2.0",
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:adobe/reactor-extension-cloud-connector-edge.git"
  },
  "scripts": {
    "build": "rm -rf dist && webpack --mode production",
    "build:watch": "webpack --mode development --watch",
    "sandbox": "concurrently -k -n webpack,sandbox -c \"bgBlue,bgMagenta\" \"npm run build:watch\" \"reactor-sandbox\"",
    "package": "npm run build && npx @adobe/reactor-packager",
    "prepack": "npm test && npm run build",
    "lint": "eslint 'src/**'",
    "format": "pretty-quick",
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "update": "rm -rf node_modules package-lock.json && npm i",
    "prepare": "husky install"
  },
  "lint-staged": {
    "*.js": [
      "eslint --cache --fix"
    ]
  },
  "dependencies": {
    "@adobe/react-spectrum": "^3.21.1",
    "@spectrum-icons/illustrations": "^3.4.0",
    "@spectrum-icons/workflow": "^4.0.2",
    "classnames": "^2.3.1",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.34.2"
  },
  "devDependencies": {
    "@adobe/reactor-sandbox": "^12.4.0",
    "@babel/core": "^7.18.13",
    "@babel/eslint-parser": "^7.18.9",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-syntax-import-assertions": "^7.18.6",
    "@babel/plugin-transform-runtime": "^7.18.10",
    "@babel/preset-env": "^7.18.10",
    "@babel/preset-react": "^7.18.6",
    "@babel/runtime": "^7.18.9",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.3.0",
    "@testing-library/user-event": "^14.4.3",
    "babel-jest": "^29.0.1",
    "babel-loader": "^8.2.5",
    "camelcase": "^7.0.0",
    "capitalize": "^2.0.4",
    "concurrently": "^7.3.0",
    "css-loader": "^6.7.1",
    "eslint": "^8.23.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-header": "^3.1.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest-dom": "^4.0.2",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.31.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-testing-library": "^5.6.0",
    "file-loader": "^6.2.0",
    "fs-extra": "^10.1.0",
    "html-webpack-plugin": "^5.5.0",
    "husky": "^8.0.1",
    "jest": "^29.0.1",
    "jest-environment-jsdom": "^29.0.1",
    "jest-runner-eslint": "^1.1.0",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "pretty-quick": "^3.1.3",
    "style-loader": "^3.3.1",
    "stylus": "^0.59.0",
    "stylus-loader": "^7.0.0",
    "url-loader": "^4.1.1",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0",
    "yargs": "^17.5.1"
  }
}
